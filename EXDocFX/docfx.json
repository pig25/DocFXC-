{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "*.csproj"
          ],
          "src":"../測試DocFX",
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ]
        }
      ],
      "dest": "obj/api",
      "properties":{
          "TargetFramework": "net45"
      },
       "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "api/*.md",
          "articles/**.md",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
     "globalMetadata": {
         "_appTitle": "測試DocFX",
         "_appFooter":"© Pig25 2020",
         "_appLogoPath":"images/pigwhite50px.png",
         "_appFaviconPath":"images/pig25px.png",
         "_enableSearch":true,
         "_enableNewTab":true,
         "_disableNavbar":false,
         "_disableBreadcrumb":false,
         "_disableToc":false,
         "_disableAffix":false,
         "_disableContribution":false
    },
    "dest": "_site",
    "template": [
      "statictoc"
    ]
  }
}